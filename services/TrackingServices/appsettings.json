{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "KeyVault": {
    "VaultUri": "https://useractivitykv.vault.azure.net/"
  },
  "CosmosDb": {
    "DatabaseName": "UserActivityTracking",
    "ContainerName": "ActivityEvents"
  },
  "ServiceBus": {
    "QueueName": "user-activity-events"
  },

  "JwtSettings": {
    "Authority": "https://your-identity-provider.com",
    "Audience": "your-api-audience"
  },
  "AllowedHosts": "*"
}
